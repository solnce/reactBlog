{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "arrow-body-style": "off",
      "jsx-a11y/label-has-for": 0,
      "react/jsx-uses-vars": 1,
      "import/no-unresolved": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
}
